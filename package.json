{
  "name": "chrome-extension-docs-checker",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "scrap": "node dist/scrap.js",
    "check:prettier": "prettier --check .",
    "check:lint": "oxlint",
    "check:ts": "tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:lint && pnpm check:ts",
    "fix:prettier": "prettier --log-level warn --write .",
    "fix:lint": "pnpm check:lint --fix --fix-suggestions",
    "fix:all": "pnpm fix:prettier && pnpm fix:lint"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "0.0.4",
    "@types/node": "22.16.5",
    "cheerio": "1.1.0",
    "oxlint": "1.11.0",
    "path": "0.12.7",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  }
}
